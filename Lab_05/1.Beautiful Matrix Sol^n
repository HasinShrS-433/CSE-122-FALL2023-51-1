#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    int a;
    for(int i=1; i<=5; i++) ///row
    {
        for(int j=1; j<=5; j++) /// col
        {
            cin>>a;
            if(a==1)
            {
                cout<<abs(3-i)+abs(3-j)<<endl;
            }
        }
    }
    return 0;
}
