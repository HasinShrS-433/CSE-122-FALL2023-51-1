#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,cnt=0,m;
    string s;
    cin>>s;
   n=s.size();
     sort(s.begin(),s.end());
    for(int i=0; i<n; i++)
    {
        if (s[i]!=s[i+1])
            cnt++;
    }

   if(cnt%2==0){
    cout<<"CHAT WITH HER!"<<endl;
   }
   else
    cout<<"IGNORE HIM!"<<endl;
    return 0;
}
