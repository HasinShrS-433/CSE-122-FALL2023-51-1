#include<iostream>
using namespace std;
int main()
{
    int n,X=0;
    cin>>n;
    string a;
    for(int i=0; i<n; i++)
    {
        cin>>a;
        if(a=="X++"||a=="++X")
        {
            X=X+1;
        }
        else if(a=="X--"||a=="--X")
        {
            X=X-1;
        }
    }
    cout<<X<<endl;
    return 0;
}
